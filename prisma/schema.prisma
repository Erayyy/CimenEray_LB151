generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model highscore {
  id     Int    @id @default(autoincrement())
  score  String @db.VarChar(255)
  player String @db.VarChar(255)
}

model phrase {
  id       Int    @id @default(autoincrement())
  phrase   String @db.VarChar(255)
  category String @db.VarChar(255)
}
